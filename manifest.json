{
  "name": "Amazon Chotto Matte",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Amazonでカートに入れるボタンを押すとウザいポップアップを表示してくれる拡張機能",
  "content_scripts": [
    {
      "matches": ["http://www.amazon.co.jp/*"],
      "js": ["jquery-2.1.1.min.js","script.js"],
      "run_at": "document_start"
    }
  ]
}